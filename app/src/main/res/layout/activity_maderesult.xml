                    android:id="@+id/maderesult_btn_main"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Main" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="시간표 결과"
